{
    "name": "upgplc/php-clientlibrary",
    "description": "The Client Library for Payco",
    "keywords": ["payco", "library"],
    "require": {
        "php": ">=5.3",
        "monolog/monolog": "1.17.2",
        "siriusphp/validation": "2.x"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.6",
        "fzaninotto/faker": "1.x",
        "fabpot/php-cs-fixer": "~1.7"
    },
    "autoload": {
        "psr-4": {
            "Upg\\Library\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Upg\\Library\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
